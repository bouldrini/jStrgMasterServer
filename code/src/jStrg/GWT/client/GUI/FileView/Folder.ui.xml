<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'>
    <ui:style>

        .fileNav ul {
            list-style-type: none;
            margin: 0;
            padding: 0;
            overflow: hidden;
            background-color: #24262d;

        }

        .fileNav li {
            float: left;
        }

        .fileNav li a {
            display: block;
            color: #fff;
            text-align: center;
            padding: 5px 5px;
            text-decoration: none;
            background-color: #24262d;

        }

        .fileNav li a:hover {
            cursor: default;

            color: #f7af3e;

        }
    </ui:style>
    <g:HTMLPanel addStyleNames="{style.fileNav}">
        <ul>
            <li>
                <g:Hyperlink ui:field="back">‚Üê</g:Hyperlink>
            </li>
            <li>
                <g:Hyperlink ui:field="newFolder">New Folder</g:Hyperlink>
            </li>
            <li>
                <g:Hyperlink ui:field="newFile">Add new File</g:Hyperlink>
            </li>
        </ul>
        <g:HTMLPanel ui:field="content">

        </g:HTMLPanel>
    </g:HTMLPanel>
</ui:UiBinder>